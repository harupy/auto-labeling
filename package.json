{
  "name": "labeler",
  "version": "0.0.0",
  "private": true,
  "description": "A bot to automate issue labeling",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc && ncc build",
    "format:js": "eslint --fix src/**/*.ts",
    "format:non-js": "prettier --write \"./**/*.{md,yml,json}\"",
    "lint:js": "eslint src/**/*.ts",
    "lint:non-js": "prettier --check \"./**/*.{md,yml,json}\"",
    "test": "jest --verbose --colors"
  },
  "dependencies": {},
  "devDependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.1",
    "@octokit/types": "^9.1.4",
    "@types/jest": "^29.5.1",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vercel/ncc": "^0.36.1",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  }
}
